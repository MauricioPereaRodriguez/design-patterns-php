{
    "name": "styde/adapter",
    "description": "An example of the Adapter Pattern",
    "type": "project",
    "repositories": [
        { "type": "path", "url": "../fake-libraries/vimeo" },
        { "type": "path", "url": "../fake-libraries/youtube" }
    ],
    "require": {
        "fake/vimeo": "dev-master",
        "fake/youtube": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Duilio Palacios",
            "email": "duilio@styde.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "files": ["src/config.php"],
        "psr-4": {"Styde\\Adapter\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Styde\\Tests\\": "tests/"}
    }
}
